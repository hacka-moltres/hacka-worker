    "tslib": "1.9.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.12",
    "@types/dotenv": "^8.2.0",
    "@types/node": "12.0.7",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "eslint": "5.16.0",
    "husky": "2.4.0",
    "ts-node-dev": "1.0.0-pre.39",
    "typescript": "3.5.1",
    "watch": "1.0.2"
